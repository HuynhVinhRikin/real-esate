<script>
    var listInput = document.querySelectorAll('.eventFocus');

    // khi click vào input thì mất placeholder
    listInput.forEach(input => {
        var valuePlaceholder;
        input.addEventListener('focus', () => {
            valuePlaceholder = input.placeholder
            input.placeholder = ""
        });
        input.addEventListener('blur', () => {
            input.placeholder = valuePlaceholder
        })
    })
</script>