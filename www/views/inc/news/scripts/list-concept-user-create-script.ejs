    else if (link.match(/ca-nhan\/yeu-thich/)) return 'caNhanYeuThich';
    else if (link.match(/tim-ban-trai/)) return 'timBanTrai';
    else if (link.match(/tim-ban-gai/)) return 'timBanGai';
    else if (link === hostName) return 'TrangChu'
    else return ''
  }

