<div class="wao-slider-item">
    <div class="row">
       <div class="col-lg-5">
          <div class="img">
                <a href="/buoi-hen-ho/<%=itemUserRefConcept.concept.slug && itemUserRefConcept.concept.slug%>">
                    <img src="/files/users/<%if(itemUserRefConcept.concept && itemUserRefConcept.concept.image){%><%=itemUserRefConcept.concept.image[0]%><%}%>
                    " alt="
                    <%if(itemUserRefConcept.concept && itemUserRefConcept.concept.title){%>
                        <%=itemUserRefConcept.concept.title%>
                    <%}%>
                   ">
                </a>
          </div>
       </div>
       <div class="col-lg-7">
          <div class="card-user" style="margin-bottom: 10px;">
             <div class="avatar">
                <%let objUser = itemUserRefConcept;%>
                <%-include ../image_RENDER-header.ejs%>
             </div>
             <div class="user-info">
                <p class="name"> 
                    <a href="/nguoi-dung/thong-tin/<%= itemUserRefConcept.username %>">
                        <%if(itemUserRefConcept.concept && itemUserRefConcept && itemUserRefConcept.fullname){%>
                            <%let user = itemUserRefConcept;%>
                            <%- include ../fullname_SPLIT.ejs%>
                        <%}%>
                        <!-- <%if(itemUserRefConcept.concept && itemUserRefConcept && itemUserRefConcept.age){%>
                            <%if(itemUserRefConcept.age !== 0){%>
                                <%=itemUserRefConcept.age%>
                            <%}%>
                        <%}%> -->
                    </a>
                </p>
                <ul class="meta">
                   <li>
                       <span>
                           <img src="/template/front-end-new/img/date-detail/eye_icon.svg" alt="icon">
                        </span>
                        <%if(itemUserRefConcept.concept && itemUserRefConcept && itemUserRefConcept.amountView){%>
                            <%=itemUserRefConcept.amountView%>
                        <%}%>
                   </li>
                   <li>
                        <span>
                           <img src="/template/front-end-new/img/date-detail/heart_icon.svg" alt="icon">
                        </span>
                        <%if(itemUserRefConcept.concept && itemUserRefConcept && itemUserRefConcept.usersLikedMe){%>
                            <%=itemUserRefConcept.usersLikedMe.length%>
                        <%}%>
                   </li>
                   <li>
                        <span>
                           <img src="/template/front-end-new/img/home/location_icon.svg" alt="icon">
                        </span>
                        <%if(provinces){%>
							<%if(itemUserRefConcept && !Number.isNaN(Number(itemUserRefConcept.province))){%>
								<% var infoProvince = provinces[itemUserRefConcept.province]%>
								<%if(infoProvince){%>
                                    <%=infoProvince.name%>
								<%}else{%>
								<%}%>
							<%}else{%>
							<%}%>
						<%}%>
                   </li>
                </ul>
             </div>
             <a href="" class="wao-like-btn wao-popup-open likeConcept 
             <%if(infoUser){%>
                <%let arrUserLikedConcept = itemUserRefConcept.concept.favorites.join('_');%>	
                <%if(arrUserLikedConcept.includes(infoUser._id)){%>
                active
                <%}else{%>
                <%}%>
                <%}else{%>
                <%}%>" _conceptid="<%=itemUserRefConcept.concept._id%>" data="wao-favorite-popup">
                <svg width="18" height="16" viewBox="0 0 18 16" xmlns="http://www.w3.org/2000/svg">
                    <path d="M9 16C8.74374 16 8.49669 15.9072 8.30415 15.7388C7.577 15.1035 6.87593 14.5065 6.2574 13.9799L6.25424 13.9772C4.44081 12.4332 2.87485 11.0998 1.78528 9.7863C0.567307 8.31791 0 6.92567 0 5.40473C0 3.92701 0.507156 2.56372 1.42795 1.56581C2.35973 0.556101 3.63826 0 5.02844 0C6.06747 0 7.01903 0.3282 7.8566 0.975406C8.2793 1.3021 8.66245 1.70192 9 2.16828C9.33769 1.70192 9.7207 1.3021 10.1435 0.975406C10.9811 0.3282 11.9327 0 12.9717 0C14.3617 0 15.6404 0.556101 16.5722 1.56581C17.493 2.56372 18 3.92701 18 5.40473C18 6.92567 17.4328 8.31791 16.2149 9.78616C15.1253 11.0998 13.5595 12.433 11.7463 13.9769C11.1267 14.5043 10.4245 15.1023 9.69571 15.739C9.50331 15.9072 9.25612 16 9 16Z"></path>
                </svg>
            </a>

          </div>
          <!-- <hr style="margin-bottom: 10px;border-color: rgba(8, 0, 0, 0.1);"> -->

          <div class="card-content">
              <!--
             <p class="wao-pc">
                <span class="wao-label">
                    <%if(constants && constants.prayers){%>
                        <%let indexPrayerConcept = constants.prayers.findIndex(prayer => Object.is(Number(prayer.value), Number(itemUserRefConcept.concept.prayer)));%>
                        <%=constants.prayers[indexPrayerConcept].text%>
                    <%}%>
                </span>
                <span class="wao-label wao-label-yellow">
                    <%if(itemUserRefConcept.concept && itemUserRefConcept.concept.participants.length > 0){%>
                        <%=itemUserRefConcept.concept.participants.length%> đăng ký
                    <%}%>
                </span>
             </p>
             -->
             <p class="title" style="margin-bottom: 15px; line-height: 26px;">
                <%if(itemUserRefConcept.concept && itemUserRefConcept.concept.title){%>
                    <%=itemUserRefConcept.concept.title%>
                <%}%>
             </p>
             <ul>
                <li><i><img src="/template/front-end-new/img/list-by-user/calendar_icon_2.svg" alt="icon"></i>
                    <span>
                       <b class="label">Ngày diễn ra:</b>
                       <br>
                       <%if(itemUserRefConcept.concept && itemUserRefConcept.concept.time){%>
                            <%= moment(itemUserRefConcept.concept.time).format('L');%>
                        <%}%>
                    </span>
                </li>
                <li class="large">
                    <i>
                        <img src="/template/front-end-new/img/list-by-user/location_icon.svg" alt="icon">
                    </i>
                    <span>
                        <b class="label">Địa điểm:</b>
                        <br>
                        <%if(itemUserRefConcept.concept && itemUserRefConcept.concept.address){%>
                            <%var conceptNew = itemUserRefConcept.concept.address.filter(Boolean);%>
                            <%=conceptNew[0]%> 
                            <%if(conceptNew.length-1 > 0){%>
                                (<%=conceptNew.length-1%> địa điểm khác)
                            <%}%>
                        <%}%>
                    </span>
                </li>
                <li>
                    <i><img src="/template/front-end-new/img/list-by-user/person_icon.svg" alt="icon"></i>
                   <span>
                       <b class="label">Đối tượng:</b>
                       <br>
                       <% if (itemUserRefConcept && itemUserRefConcept.concept && itemUserRefConcept.concept.focusGender && itemUserRefConcept.concept.focusGender == 1) { %>
                        Nam
                        <% }else{ %>
                            Nữ
                        <% } %>
                        &nbsp;<%=_lodash.get(itemUserRefConcept, 'concept.focusAge.from', '...')%> - <%=_lodash.get(itemUserRefConcept, 'concept.focusAge.to', '...')%>
                    </span>
                </li>
                <!-- <li>
                    <i><img src="/template/front-end-new/img/home/save_money_icon.svg"alt="icon"></i>
                   <span>
                        <b class="label">Tổng chi phí:</b>
                        <br>
                        <b>
                            <%if(itemUserRefConcept.concept && itemUserRefConcept.concept.cost){%>
                                <%= formatCurrency('###,###.', itemUserRefConcept.concept.cost)%> VNĐ
                            <%}%>
                        </b>
                    </span>
                </li> -->
                <li>
                    <i><img src="/template/front-end-new/img/home/save_money_icon.svg"alt="icon"></i>
                   <b>
                    <%if(constants && constants.prayers){%>
                        <%let indexPrayerConcept = constants.prayers.findIndex(prayer => Object.is(Number(prayer.value), Number(itemUserRefConcept.concept.prayer)));%>
                        <%=constants.prayers[indexPrayerConcept].text%>
                    <%}%>
                   </b>
                </li>
             </ul>
             <p class="desc" style="overflow: hidden;
                text-overflow: ellipsis;
                display: -webkit-box;
                -webkit-line-clamp: 2;
                -webkit-box-orient: vertical;">
                <%if(itemUserRefConcept.concept && itemUserRefConcept.concept.description){%>
                    <%=itemUserRefConcept.concept.description%>
                <%}%>
             </p>
             <p class="text-center">
                <a href="/buoi-hen-ho/<%=itemUserRefConcept.concept.slug && itemUserRefConcept.concept.slug%>" class="wao-btn wao-btn-outline-b">
                Xem chi tiết
                </a>
                <%let flagShowButtonNotIsAuthor = true;%>
                <%if(Number(itemUserRefConcept.concept.status) == 0){%>
                    <%if(isLogin){%>
                        <%if(infoUser._id.toString() == `${itemUserRefConcept.concept.author._id}`){%>
                            <%flagShowButtonNotIsAuthor = false;%>
                        <%}%>
                    <%}%>
                    <%if(!flagShowButtonNotIsAuthor) {%>
                        
                    <%}else{%>
                        <% let checkParticipant = infoUser && itemUserRefConcept.participant_concept.length && itemUserRefConcept.participant_concept.find(item => item.participant.toString() == infoUser._id.toString());%>
                        <%if(!checkParticipant) {%> 
                            <a _conceptID='<%=itemUserRefConcept.concept._id%>' _authorID='<%=itemUserRefConcept._id%>' href="#" class="wao-btn wao-btn-bubble wao-btn-join wao-popup-open requestParticipantion" data="wao-enjoy-date-popup" >
                                Tham gia
                            </a>
                        <% } else {%>
                        <a class="wao-btn wao-btn-bubble wao-popup-open requestParticipantion" href="#" _conceptID='<%=itemUserRefConcept.concept._id%>' _authorID='<%=itemUserRefConcept._id%>'  data="wao-enjoy-date-popup">
                            Đã tham gia 
                        </a>
                        <% } %>
                    <%}%>
                <%}%>
             </p>
          </div>
       </div>
    </div>
 </div>





